{
    "name": "BicycleNetwork1",
    "baudrate": 19200,
    "master": "BodyComputer",
    "nodes": {
        "BodyComputer": {
            "address": "0x0",
            "subscribes": [
                "SpeedStatus",
                "RideStatus",
                "RearLightStatus",
                "FrontLightStatus"
            ],
            "publishes": [
                "LightSynchronization",
                "RearLightSetting",
                "FrontLightSetting"
            ]
        },
        "RotorSensor": {
            "address": "0x1",
            "subscribes": [
            ],
            "publishes": [
                "SpeedStatus",
                "RideStatus",
                "SpeedDebug"
            ]
        },
        "RearLight": {
            "address": "0x03",
            "subscribes": [
                "SpeedStatus",
                "LightSynchronization",
                "RearLightSetting"
            ],
            "publishes": [
                "RearLightStatus",
                "RearLightDebug"
            ]
        },
        "FrontLight": {
            "address": "0x04",
            "subscribes": [
                "SpeedStatus",
                "LightSynchronization",
                "FrontLightSetting"
            ],
            "publishes": [
                "FrontLightStatus",
                "FrontLightDebug"
            ]
        }
    },
    "requests": {
        "SpeedStatus": {
            "id": "0x1000",
            "size": 5,
            "layout": {
                "GlobalSpeed": {
                    "offset": 0,
                    "width": 16,
                    "initial": 0.0,
                    "encoder": "GlobalSpeedEncoder"
                },
                "GlobalSpeedState": {
                    "offset": 16,
                    "width": 2,
                    "initial": "Error",
                    "encoder": "GlobalSpeedStateEncoder"
                },
                "BrakeState": {
                    "offset": 18,
                    "width": 2,
                    "initial": "Disabled",
                    "encoder": "BrakeStateEncoder"
                },
                "FrontWheelSlip": {
                    "offset": 20,
                    "width": 1,
                    "initial": "NotSlipping",
                    "encoder": "WheelSlipEncoder"
                },
                "FrontWheelLockup": {
                    "offset": 21,
                    "width": 1,
                    "initial": "NoLockup",
                    "encoder": "WheelLockupEncoder"
                },
                "RearWheelSlip": {
                    "offset": 22,
                    "width": 1,
                    "initial": "NotSlipping",
                    "encoder": "WheelSlipEncoder"
                },
                "RearWheelLockup": {
                    "offset": 23,
                    "width": 1,
                    "initial": "NoLockup",
                    "encoder": "WheelLockupEncoder"
                }
            }
        },
        "RideStatus": {
            "id": "0x1001",
            "size": 8,
            "layout": {
                "RideDistance": {
                    "offset": 0,
                    "width": 32,
                    "initial": 0,
                    "encoder": "RideDistanceEncoder"
                },
                "MaxSpeed": {
                    "offset": 32,
                    "width": 16,
                    "initial": 0.0,
                    "encoder": "GlobalSpeedEncoder"
                },
                "AverageSpeed": {
                    "offset": 48,
                    "width": 16,
                    "initial": 0.0,
                    "encoder": "GlobalSpeedEncoder"
                }
            }
        },
        "SpeedDebug": {
            "id": "0x1003",
            "size": 7,
            "layout": {
                "FrontSpeed": {
                    "offset": 0,
                    "width": 16,
                    "initial": 0.0,
                    "encoder": "GlobalSpeedEncoder"
                },
                "RearSpeed": {
                    "offset": 16,
                    "width": 16,
                    "initial": 0.0,
                    "encoder": "GlobalSpeedEncoder"
                },
                "FrontWheelSpeedState": {
                    "offset": 32,
                    "width": 3,
                    "initial": "Off",
                    "encoder": "ChannelSpeedStateEncoder"
                },
                "RearWheelSpeedState": {
                    "offset": 35,
                    "width": 3,
                    "initial": "Off",
                    "encoder": "ChannelSpeedStateEncoder"
                },
                "IntertialSensorState": {
                    "offset": 38,
                    "width": 2,
                    "initial": 0
                }
            }
        },
        "LightSynchronization": {
            "id": "0x1040",
            "size": 2,
            "layout": {
                "TargetBrightness": {
                    "offset": 0,
                    "width": 8,
                    "initial": 0,
                    "encoder": "LightBrightnessEncoder"
                },
                "LightMode": {
                    "offset": 8,
                    "width": 2,
                    "initial": "Adaptive",
                    "encoder": "LightModeEncoder"
                },
                "TurnSignalDirection": {
                    "offset": 10,
                    "width": 2,
                    "initial": "Disabled",
                    "encoder": "TurnSignalDirectionEncoder"
                },
                "StrobeSignal": {
                    "offset": 12,
                    "width": 1,
                    "initial": "Off",
                    "encoder": "StrobeSignalEncoder"
                },
                "AntiStrobeSignal": {
                    "offset": 13,
                    "width": 1,
                    "initial": "Off",
                    "encoder": "StrobeSignalEncoder"
                },
                "Reserved": {
                    "offset": 14,
                    "width": 2,
                    "initial": 0
                }
            }
        },
        "RearLightSetting": {
            "id": "0x1050",
            "size": 1,
            "layout": {
                "Behavior": {
                    "offset": 0,
                    "width": 2,
                    "initial": "Default",
                    "encoder": "LightBehaviorEncoder"
                },
                "BrakeLightMode": {
                    "offset": 2,
                    "width": 1,
                    "initial": "Default",
                    "encoder": "GenericModeSwitchEncoder"
                },
                "TurnSignalLightMode": {
                    "offset": 3,
                    "width": 1,
                    "initial": "Default",
                    "encoder": "GenericModeSwitchEncoder"
                },
                "Reserved": {
                    "offset": 4,
                    "width": 4,
                    "initial": 0
                }
            }
        },
        "RearLightStatus": {
            "id": "0x1051",
            "size": 1,
            "layout": {
                "TailLightStatus": {
                    "offset": 0,
                    "width": 2,
                    "initial": "Off",
                    "encoder": "LightStatusEncoder"
                },
                "BrakeLightStatus": {
                    "offset": 2,
                    "width": 2,
                    "initial": "Off",
                    "encoder": "LightStatusEncoder"
                },
                "TurnSignalLightStatus": {
                    "offset": 4,
                    "width": 2,
                    "initial": "Off",
                    "encoder": "LightStatusEncoder"
                },
                "ThermalStatus": {
                    "offset": 6,
                    "width": 2,
                    "initial": "NotMeasured",
                    "encoder": "ThermalStatusEncoder"
                }
            }
        },
        "RearLightDebug": {
            "id": "0x1053",
            "size": 2,
            "layout": {
                "SensorErrorCode": {
                    "offset": 0,
                    "width": 8,
                    "initial": 0
                },
                "EcuTemperature": {
                    "offset": 8,
                    "width": 8,
                    "initial": 0,
                    "encoder": "TemperatureEncoder"
                }
            }
        },
        "FrontLightSetting": {
            "id": "0x1060",
            "size": 1,
            "layout": {
                "Behavior": {
                    "offset": 0,
                    "width": 2,
                    "initial": "Default",
                    "encoder": "LightBehaviorEncoder"
                },
                "BeamForming": {
                    "offset": 2,
                    "width": 1,
                    "initial": "Default",
                    "encoder": "GenericModeSwitchEncoder"
                },
                "TurnSignalLightMode": {
                    "offset": 3,
                    "width": 1,
                    "initial": "Default",
                    "encoder": "GenericModeSwitchEncoder"
                },
                "HighBeam": {
                    "offset": 4,
                    "width": 1,
                    "initial": "Off",
                    "encoder": "GenericSwitchEncoder"
                },
                "Reserved": {
                    "offset": 5,
                    "width": 3,
                    "initial": 0
                }
            }
        },
        "FrontLightStatus": {
            "id": "0x1061",
            "size": 1,
            "layout": {
                "MainBeamStatus": {
                    "offset": 0,
                    "width": 2,
                    "initial": "Off",
                    "encoder": "LightStatusEncoder"
                },
                "Reserved": {
                    "offset": 2,
                    "width": 4,
                    "initial": 0
                },
                "ThermalStatus": {
                    "offset": 6,
                    "width": 2,
                    "initial": "NotMeasured",
                    "encoder": "ThermalStatusEncoder"
                }
            }
        },
        "FrontLightDebug": {
            "id": "0x1062",
            "size": 2,
            "layout": {
                "DriveTemperature": {
                    "offset": 0,
                    "width": 8,
                    "initial": 0,
                    "encoder": "TemperatureEncoder"
                },
                "McuTemperature": {
                    "offset": 8,
                    "width": 8,
                    "initial": 0,
                    "encoder": "TemperatureEncoder"
                }
            }
        }
    },
    "encoders": {
        "GlobalSpeedEncoder": {
            "type": "formula",
            "scale": 0.1,
            "offset": 0.0,
            "unit": "km/h"
        },
        "GlobalSpeedStateEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Ok",
                "1": "SlowResponse",
                "2": "Unreliable",
                "3": "Error"
            }
        },
        "ChannelSpeedStateEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Ok",
                "1": "UNUSED",
                "2": "SensorWarning",
                "3": "SpeedUnreliable",
                "4": "SensorError",
                "5": "OutputOpen",
                "6": "OutputShorted",
                "7": "Off"
            }
        },
        "BrakeStateEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Disabled",
                "1": "NotBraking",
                "2": "Braking",
                "3": "UNUSED"
            }
        },
        "WheelSlipEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "NotSlipping",
                "1": "Slipping"
            }
        },
        "WheelLockupEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "NoLockup",
                "1": "Lockup"
            }
        },
        "RideDistanceEncoder": {
            "type": "formula",
            "scale": 1,
            "offset": 0,
            "unit": "m"
        },
        "GenericStatusFlag": {
            "type": "mapping",
            "mapping": {
                "0": "Ok",
                "1": "Error"
            }
        },
        "GenericSwitchEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Off",
                "1": "On"
            }
        },
        "GenericModeSwitchEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Default",
                "1": "Off"
            }
        },
        "TemperatureEncoder": {
            "type": "formula",
            "scale": 1,
            "offset": -40,
            "unit": "Â°C"
        },
        "LightBrightnessEncoder": {
            "type": "formula",
            "scale": 1,
            "offset": 0
        },
        "LightModeEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Adaptive",
                "1": "Standard",
                "2": "Emergency",
                "3": "Off"
            }
        },
        "TurnSignalDirectionEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Disabled",
                "1": "Warning",
                "2": "Left",
                "3": "Right"
            }
        },
        "StrobeSignalEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "On",
                "1": "Off"
            }
        },
        "LightBehaviorEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Default",
                "1": "Solid",
                "2": "Blink",
                "3": "UNUSED"
            }
        },
        "LightStatusEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Off",
                "1": "Ok",
                "2": "PartialError",
                "3": "Error"
            }
        },
        "ThermalStatusEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "NotMeasured",
                "1": "Ok",
                "2": "Derating",
                "3": "Shutdown"
            }
        }
    },
    "schedules": {

    }
}
