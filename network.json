    "encoders": {
        "GlobalSpeedEncoder": {
            "type": "formula",
            "scale": 0.1,
            "offset": 0.0,
            "unit": "km/h"
        },
        "GlobalSpeedStateEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Ok",
                "1": "SlowResponse",
                "2": "Unreliable",
                "3": "Error"
            }
        },
        "ChannelSpeedStateEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Ok",
                "1": "UNUSED",
                "2": "SensorWarning",
                "3": "SpeedUnreliable",
                "4": "SensorError",
                "5": "OutputOpen",
                "6": "OutputShorted",
                "7": "Off"
            }
        },
        "BrakeStateEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Disabled",
                "1": "NotBraking",
                "2": "Braking",
                "3": "UNUSED"
            }
        },
        "WheelSlipEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "NotSlipping",
                "1": "Slipping"
            }
        },
        "WheelLockupEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "NoLockup",
                "1": "Lockup"
            }
        },
        "RideDistanceEncoder": {
            "type": "formula",
