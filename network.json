        },
        "FrontLightBrightnessDebug": {
            "id": "0x1063",
            "size": 2,
            "layout": {
                "Brightness": {
                    "offset": 0,
                    "width": 16,
                    "initial": 0
                }
            }
        },
        "FrontLightTemperatureDebug": {
            "id": "0x1064",
            "size": 2,
            "layout": {
                "DriveTemperature": {
                    "offset": 0,
                    "width": 8,
                    "initial": 0,
                    "encoder": "TemperatureEncoder"
                },
                "McuTemperature": {
                    "offset": 8,
                    "width": 8,
                    "initial": 0,
                    "encoder": "TemperatureEncoder"
                }
            }
        }
    },
    "encoders": {
        "SpeedEncoder": {
            "type": "formula",
            "scale": 0.1,
            "offset": 0.0,
            "unit": "km/h"
        },
        "SpeedStateEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Ok",
                "1": "SlowResponse",
                "2": "Unreliable",
                "3": "Error"
            }
        },
        "SpeedSensorStateEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Ok",
                "1": "UNUSED1",
                "2": "UNUSED2",
                "3": "SensorWarning",
                "4": "SensorError",
                "5": "OutputOpen",
                "6": "OutputShorted",
                "7": "Off"
            }
        },
        "BrakeStateEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Disabled",
                "1": "NotBraking",
                "2": "Braking",
                "3": "UNUSED"
            }
        },
        "WheelSlipEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "NotSlipping",
                "1": "Slipping"
            }
        },
        "WheelLockupEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "NoLockup",
                "1": "Lockup"
            }
        },
        "CadenceEncoder": {
            "type": "formula",
            "scale": 1,
            "offset": 0,
            "unit": "rpm"
        },
        "CadenceStatusEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "NotAvailable",
                "1": "Ok",
                "2": "Coasting",
                "3": "Error"
            }
        },
        "GearStatusEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Unknown",
                "1": "Ok",
                "2": "OutOfRange",
                "3": "Error"
            }
        },
        "TimeDurationEncoder": {
            "type": "formula",
            "scale": 1,
            "offset": 0,
            "unit": "sec"
        },
        "DistanceEncoder": {
            "type": "formula",
            "scale": 1,
            "offset": 0,
            "unit": "m"
        },
        "DistanceStatusEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "NotAvailable",
                "1": "Ok",
                "2": "Error",
                "3": "PermanentError"
            }
        },
        "RideStatusEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "NotStarted",
                "1": "Active",
                "2": "Idle",
                "3": "Paused"
            }
        },
        "AltitudeEncoder": {
            "type": "formula",
            "scale": 1,
            "offset": -1000,
            "unit": "m"
        },
        "GradeEncoder": {
            "type": "formula",
            "scale": 1,
            "offset": -128,
            "unit": "%"
        },
        "RoadQualityEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "NotMeasured",
                "1": "Flat",
                "2": "Rough",
                "3": "VeryRough"
            }
        },
        "ITPMSEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Stopped",
                "1": "Running",
                "2": "FrontPressureLow",
                "3": "RearPressureLow"
            }
        },
        "GenericStatusFlag": {
            "type": "mapping",
            "mapping": {
                "0": "Ok",
                "1": "Error"
            }
        },
        "GenericSwitchEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Off",
                "1": "On"
            }
        },
        "GenericModeSwitchEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Default",
                "1": "Off"
            }
        },
        "TemperatureEncoder": {
            "type": "formula",
            "scale": 1,
            "offset": -40,
            "unit": "Â°C"
        },
        "LightBrightnessEncoder": {
            "type": "formula",
            "scale": 1,
            "offset": 0
        },
        "LightModeEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Adaptive",
                "1": "Standard",
                "2": "Emergency",
                "3": "Off"
            }
        },
        "TurnSignalDirectionEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Disabled",
                "1": "Warning",
                "2": "Left",
                "3": "Right"
            }
        },
        "StrobeSignalEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "On",
                "1": "Off"
            }
        },
        "LightBehaviorEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Default",
                "1": "Solid",
                "2": "Blink",
                "3": "UNUSED"
            }
        },
        "LightStatusEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "Off",
                "1": "Ok",
                "2": "PartialError",
                "3": "Error"
            }
        },
        "ThermalStatusEncoder": {
            "type": "mapping",
            "mapping": {
                "0": "NotMeasured",
                "1": "Ok",
                "2": "Derating",
                "3": "Shutdown"
            }
        }
    },
    "schedules": {
        "NormalSchedule": {
            "type": "priority-aging",
            "slots": "variable",
            "delay": 0.05,
            "phase": "adjusted",
            "entries": [
                {"type": "request", "request": "SpeedStatus", "cycle": 0, "maxAge": 2},
                {"type": "request", "request": "LightSynchronization", "cycle": 1, "maxAge": 3},
                {"type": "request", "request": "RideStatus", "cycle": 5, "maxAge": 20},
                {"type": "request", "request": "DrivetrainStatus", "cycle": 5, "maxAge": 30},
                {"type": "request", "request": "RoadStatus", "cycle": 15, "maxAge": 30},
                {"type": "request", "request": "RideStatistics", "cycle": 15, "maxAge": 30},
                {"type": "request", "request": "FrontLightStatus", "cycle": 8, "maxAge": 15},
                {"type": "request", "request": "FrontLightSetting", "cycle": 8, "maxAge": 15},
                {"type": "request", "request": "RearLightSetting", "cycle": 8, "maxAge": 15},
                {"type": "request", "request": "RearLightStatus", "cycle": 8, "maxAge": 15}
            ]
        },
        "RotorSensorSchedule": {
            "type": "fixed",
            "slots": "variable",
            "delay": 0.05,
            "entries": [
                {"type": "request", "request": "SpeedStatus"},
                {"type": "request", "request": "DrivetrainStatus"},
                {"type": "request", "request": "RideStatus"},
                {"type": "request", "request": "RoadStatus"},
                {"type": "request", "request": "RideStatistics"},
                {"type": "request", "request": "RotorSensorPressureDebug"},
                {"type": "request", "request": "RotorSensorMotionDebug"},
                {"type": "request", "request": "RotorSensorSpeedDebug"}
            ]
        },
        "LightSchedule": {
            "type": "fixed",
            "slots": "variable",
            "delay": 0.05,
            "entries": [
                {"type": "request", "request": "LightSynchronization"},
                {"type": "request", "request": "RearLightSetting"},
                {"type": "request", "request": "RearLightStatus"},
                {"type": "request", "request": "FrontLightSetting"},
                {"type": "request", "request": "FrontLightStatus"}
            ]
        },
        "RearLightSchedule": {
            "type": "fixed",
            "slots": "variable",
            "delay": 0.05,
            "entries": [
                {"type": "request", "request": "LightSynchronization"},
                {"type": "request", "request": "RearLightSetting"},
                {"type": "request", "request": "RearLightStatus"},
                {"type": "request", "request": "RearLightBrightnessDebug"},
                {"type": "request", "request": "RearLightTemperatureDebug"},
                {"type": "request", "request": "RearLightMotionDebug"},
                {"type": "opstatus", "node": "RearLight"},
                {"type": "pwrstatus", "node": "RearLight"},
                {"type": "serial", "node": "RearLight"},
                {"type": "swversion", "node": "RearLight"}
            ]
        },
        "FrontLightSchedule": {
            "type": "fixed",
            "slots": "variable",
            "delay": 0.05,
            "entries": [
                {"type": "request", "request": "LightSynchronization"},
                {"type": "request", "request": "FrontLightSetting"},
                {"type": "request", "request": "FrontLightStatus"},
                {"type": "request", "request": "FrontLightTemperatureDebug"},
                {"type": "request", "request": "FrontLightBrightnessDebug"},
                {"type": "opstatus", "node": "FrontLight"},
                {"type": "pwrstatus", "node": "FrontLight"},
                {"type": "serial", "node": "FrontLight"},
                {"type": "swversion", "node": "FrontLight"}
            ]
        }
    }
}
