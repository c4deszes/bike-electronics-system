{
    "name": "BicycleNetwork1",
    "baudrate": 19200,
    "master": "BodyComputer",
    "nodes": {
        "BodyComputer": {
            "address": "0x0",
            "subscribes": [
                "SpeedStatus",
                "DrivetrainStatus",
                "RideStatus",
                "RoadStatus",
                "RideStatistics",
                "RearLightStatus",
                "FrontLightStatus"
            ],
            "publishes": [
                "LightSynchronization",
                "RearLightSetting",
                "FrontLightSetting"
            ]
        },
        "RotorSensor": {
            "address": "0x1",
            "subscribes": [
            ],
            "publishes": [
                "SpeedStatus",
                "DrivetrainStatus",
                "RideStatus",
                "RoadStatus",
                "RideStatistics",
                "RotorSensorPressureDebug",
                "RotorSensorMotionDebug",
                "RotorSensorSpeedDebug"
            ]
        },
        "RearLight": {
            "address": "0x03",
            "subscribes": [
                "SpeedStatus",
                "LightSynchronization",
                "RearLightSetting"
            ],
            "publishes": [
                "RearLightStatus",
                "RearLightBrightnessDebug",
                "RearLightTemperatureDebug",
                "RearLightMotionDebug"
            ]
        },
        "FrontLight": {
            "address": "0x04",
            "subscribes": [
                "SpeedStatus",
                "LightSynchronization",
                "FrontLightSetting"
            ],
            "publishes": [
                "FrontLightStatus",
                "FrontLightBrightnessDebug",
                "FrontLightTemperatureDebug"
            ]
        }
    },
    "requests": {
        "SpeedStatus": {
            "id": "0x1000",
            "size": 3,
            "layout": {
                "Speed": {
                    "offset": 0,
                    "width": 16,
                    "initial": 0.0,
                    "encoder": "SpeedEncoder"
                },
                "SpeedState": {
                    "offset": 16,
                    "width": 2,
                    "initial": "Error",
                    "encoder": "SpeedStateEncoder"
                },
                "BrakeState": {
                    "offset": 18,
                    "width": 2,
                    "initial": "Disabled",
                    "encoder": "BrakeStateEncoder"
                },
                "FrontWheelSlip": {
                    "offset": 20,
                    "width": 1,
                    "initial": "NotSlipping",
                    "encoder": "WheelSlipEncoder"
                },
                "FrontWheelLockup": {
                    "offset": 21,
                    "width": 1,
                    "initial": "NoLockup",
                    "encoder": "WheelLockupEncoder"
                },
                "RearWheelSlip": {
                    "offset": 22,
                    "width": 1,
                    "initial": "NotSlipping",
                    "encoder": "WheelSlipEncoder"
                },
                "RearWheelLockup": {
                    "offset": 23,
                    "width": 1,
                    "initial": "NoLockup",
                    "encoder": "WheelLockupEncoder"
                }
            }
        },
        "DrivetrainStatus": {
            "id": "0x1001",
            "size": 2,
            "layout": {
                "Cadence": {
                    "offset": 0,
                    "width": 8,
                    "initial": 0,
                    "encoder": "CadenceEncoder"
                },
                "CadenceStatus": {
                    "offset": 8,
                    "width": 2,
                    "initial": "NotAvailable",
                    "encoder": "CadenceStatusEncoder"
                },
                "EstimatedGear": {
                    "offset": 10,
                    "width": 4,
                    "initial": 0
                },
                "GearStatus": {
                    "offset": 14,
                    "width": 2,
                    "initial": "Unknown",
                    "encoder": "GearStatusEncoder"
                }
            }
        },
        "RideStatus": {
            "id": "0x1002",
            "size": 7,
            "layout": {
                "Duration": {
                    "offset": 0,
                    "width": 16,
                    "initial": 0
                },
                "RideStatus": {
                    "offset": 16,
                    "width": 2,
                    "initial": "NotStarted",
                    "encoder": "RideStatusEncoder"
                },
                "DistanceStatus": {
                    "offset": 18,
                    "width": 2,
                    "initial": "NotAvailable",
                    "encoder": "DistanceStatusEncoder"
                },
                "Reserved": {
                    "offset": 20,
                    "width": 4,
                    "initial": 0
                },
                "Distance": {
                    "offset": 24,
                    "width": 32,
                    "initial": 0,
                    "encoder": "DistanceEncoder"
                }
            }
        },
        "RoadStatus": {
            "id": "0x1003",
            "size": 4,
            "layout": {
                "Altitude": {
                    "offset": 0,
                    "width": 16,
                    "initial": 0,
                    "encoder": "AltitudeEncoder"
                },
                "Grade": {
                    "offset": 16,
                    "width": 8,
                    "initial": 0,
                    "encoder": "GradeEncoder"
                },
                "RoadQuality": {
                    "offset": 24,
                    "width": 2,
                    "initial": "NotMeasured",
                    "encoder": "RoadQualityEncoder"
                },
                "ITPMS": {
                    "offset": 26,
                    "width": 2,
                    "initial": "Stopped",
                    "encoder": "ITPMSEncoder"
                },
                "PressureError": {
                    "offset": 28,
                    "width": 1,
                    "initial": "Ok",
                    "encoder": "GenericStatusFlag"
                },
                "TemperatureError": {
                    "offset": 29,
                    "width": 1,
                    "initial": "Ok",
                    "encoder": "GenericStatusFlag"
                },
                "AltitudeError": {
                    "offset": 30,
                    "width": 1,
                    "initial": "Ok",
                    "encoder": "GenericStatusFlag"
                },
                "Reserved": {
                    "offset": 31,
                    "width": 1,
                    "initial": 0
                }
            }
        },
        "RideStatistics": {
            "id": "0x1004",
            "size": 8,
            "layout": {
                "TopSpeed": {
                    "offset": 0,
                    "width": 16,
                    "initial": 0.0,
                    "encoder": "SpeedEncoder"
                },
                "AverageSpeed": {
                    "offset": 16,
                    "width": 16,
                    "initial": 0.0,
                    "encoder": "SpeedEncoder"
                },
                "Elevation": {
                    "offset": 32,
                    "width": 16,
                    "initial": 0,
                    "encoder": "DistanceEncoder"
                },
                "Descent": {
                    "offset": 48,
                    "width": 16,
                    "initial": 0,
                    "encoder": "DistanceEncoder"
                }
            }
        },
        "RotorSensorPressureDebug": {
            "id": "0x1010",
            "size": 4,
            "layout": {
                "Pressure": {
                    "offset": 0,
                    "width": 32,
                    "initial": 0
                }
            }
        },
        "RotorSensorMotionDebug": {
            "id": "0x1011",
            "size": 12,
            "layout": {
                "aX": {
                    "offset": 0,
                    "width": 16,
                    "initial": 0
                },
                "aY": {
                    "offset": 16,
                    "width": 16,
                    "initial": 0
                },
                "aZ": {
                    "offset": 32,
                    "width": 16,
                    "initial": 0
                },
                "gX": {
                    "offset": 48,
                    "width": 16,
                    "initial": 0
                },
                "gY": {
                    "offset": 64,
                    "width": 16,
                    "initial": 0
                },
                "gZ": {
                    "offset": 80,
                    "width": 16,
                    "initial": 0
                }
            }
        },
        "RotorSensorSpeedDebug": {
            "id": "0x1012",
            "size": 8,
            "layout": {
                "FrontSpeed": {
                    "offset": 0,
                    "width": 16,
                    "initial": 0,
                    "encoder": "SpeedEncoder"
                },
                "RearSpeed": {
                    "offset": 16,
                    "width": 16,
                    "initial": 0,
                    "encoder": "SpeedEncoder"
                },
                "FrontSensorStatus": {
                    "offset": 32,
                    "width": 4,
                    "initial": "Ok",
                    "encoder": "SpeedSensorStateEncoder"
                },
                "RearSensorStatus": {
                    "offset": 36,
                    "width": 4,
                    "initial": "Ok",
                    "encoder": "SpeedSensorStateEncoder"
                },
                "CrankSensorStatus": {
                    "offset": 40,
                    "width": 4,
                    "initial": "Ok",
                    "encoder": "SpeedSensorStateEncoder"
                },
                "CrankPosition": {
                    "offset": 44,
                    "width": 4,
                    "initial": 0
                }
            }
        },
        "LightSynchronization": {
            "id": "0x1040",
            "size": 2,
            "layout": {
                "TargetBrightness": {
                    "offset": 0,
                    "width": 8,
                    "initial": 0,
                    "encoder": "LightBrightnessEncoder"
                },
                "LightMode": {
                    "offset": 8,
                    "width": 2,
                    "initial": "Adaptive",
                    "encoder": "LightModeEncoder"
                },
                "TurnSignalDirection": {
                    "offset": 10,
                    "width": 2,
                    "initial": "Disabled",
                    "encoder": "TurnSignalDirectionEncoder"
                },
                "StrobeSignal": {
                    "offset": 12,
                    "width": 1,
                    "initial": "Off",
                    "encoder": "StrobeSignalEncoder"
                },
                "AntiStrobeSignal": {
                    "offset": 13,
                    "width": 1,
                    "initial": "Off",
                    "encoder": "StrobeSignalEncoder"
                },
                "Reserved": {
                    "offset": 14,
                    "width": 2,
                    "initial": 0
                }
            }
        },
        "RearLightSetting": {
            "id": "0x1050",
            "size": 1,
            "layout": {
                "Behavior": {
                    "offset": 0,
                    "width": 2,
                    "initial": "Default",
                    "encoder": "LightBehaviorEncoder"
                },
                "BrakeLightMode": {
                    "offset": 2,
                    "width": 1,
                    "initial": "Default",
                    "encoder": "GenericModeSwitchEncoder"
                },
                "TurnSignalLightMode": {
                    "offset": 3,
                    "width": 1,
                    "initial": "Default",
                    "encoder": "GenericModeSwitchEncoder"
                },
                "Reserved": {
                    "offset": 4,
                    "width": 4,
                    "initial": 0
                }
            }
        },
        "RearLightStatus": {
            "id": "0x1051",
            "size": 1,
            "layout": {
                "TailLightStatus": {
                    "offset": 0,
                    "width": 2,
                    "initial": "Off",
                    "encoder": "LightStatusEncoder"
                },
                "BrakeLightStatus": {
                    "offset": 2,
                    "width": 2,
                    "initial": "Off",
                    "encoder": "LightStatusEncoder"
                },
                "TurnSignalLightStatus": {
                    "offset": 4,
                    "width": 2,
                    "initial": "Off",
                    "encoder": "LightStatusEncoder"
                },
                "ThermalStatus": {
                    "offset": 6,
                    "width": 2,
                    "initial": "NotMeasured",
                    "encoder": "ThermalStatusEncoder"
                }
            }
        },
        "RearLightBrightnessDebug": {
            "id": "0x1052",
            "size": 2,
            "layout": {
                "Brightness": {
                    "offset": 0,
                    "width": 16,
                    "initial": 0,
                    "encoder": "LightBrightnessEncoder"
                }
            }
        },
        "RearLightTemperatureDebug": {
            "id": "0x1053",
            "size": 2,
            "layout": {
                "DriveTemperature": {
                    "offset": 0,
                    "width": 8,
                    "initial": 0,
                    "encoder": "TemperatureEncoder"
                },
                "EcuTemperature": {
                    "offset": 8,
                    "width": 8,
                    "initial": 0,
                    "encoder": "TemperatureEncoder"
                }
            }
        },
        "RearLightMotionDebug": {
            "id": "0x1054",
            "size": 7,
            "layout": {
                "aX": {
                    "offset": 0,
                    "width": 16,
                    "initial": 0
                },
                "aY": {
                    "offset": 16,
                    "width": 16,
                    "initial": 0
                },
                "aZ": {
                    "offset": 32,
                    "width": 16,
                    "initial": 0
                },
                "Braking": {
                    "offset": 48,
                    "width": 1,
                    "initial": "Off",
                    "encoder": "GenericSwitchEncoder"
                },
                "Reserved": {
                    "offset": 49,
                    "width": 7,
                    "initial": 0
                }
            }
        },
        "FrontLightSetting": {
            "id": "0x1060",
            "size": 1,
            "layout": {
                "Behavior": {
                    "offset": 0,
                    "width": 2,
                    "initial": "Default",
                    "encoder": "LightBehaviorEncoder"
                },
                "BeamForming": {
                    "offset": 2,
                    "width": 1,
                    "initial": "Default",
                    "encoder": "GenericModeSwitchEncoder"
                },
                "TurnSignalLightMode": {
                    "offset": 3,
                    "width": 1,
                    "initial": "Default",
                    "encoder": "GenericModeSwitchEncoder"
                },
                "HighBeam": {
                    "offset": 4,
                    "width": 1,
                    "initial": "Off",
                    "encoder": "GenericSwitchEncoder"
                },
                "Reserved": {
                    "offset": 5,
                    "width": 3,
                    "initial": 0
                }
            }
        },
        "FrontLightStatus": {
            "id": "0x1061",
            "size": 2,
            "layout": {
                "MainBeamStatus": {
                    "offset": 0,
                    "width": 2,
                    "initial": "Off",
                    "encoder": "LightStatusEncoder"
                },
                "Reserved": {
                    "offset": 2,
                    "width": 4,
                    "initial": 0
                },
                "ThermalStatus": {
                    "offset": 6,
                    "width": 2,
