            "type": "mapping",
            "mapping": {
                "0": "Stopped",
                "1": "Running",
                "2": "FrontPressureLow",
                "3": "RearPressureLow"
            }
        },
        "GenericStatusFlag": {
            "type": "mapping",
            "mapping": {
                "0": "Ok",
                "1": "Error"
            }
        },
